    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "@react-navigation/stack": "^6.3.16",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "email-validator": "^2.0.4",
    "expo": "~48.0.6",
    "expo-status-bar": "~1.4.4",
    "jest": "^29.2.1",
    "jest-expo": "^48.0.2",
    "native-notify": "^3.0.6",
    "react": "18.2.0",
    "react-items-carousel": "^2.8.0",
